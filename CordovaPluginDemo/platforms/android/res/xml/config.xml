<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.cordovaplugindemo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SpeakNative">
        <param name="android-package" value="com.maxrocky.plugins.speaknative.SpeakNative" />
    </feature>
    <feature name="ActionSheet">
        <param name="android-package" value="nl.xservices.plugins.actionsheet.ActionSheet" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="AliPay">
        <param name="android-package" value="com.maxrocky.plugins.alipay.AliPay" />
    </feature>
    <feature name="MRBase64Image">
        <param name="android-package" value="com.maxrocky.plugins.base64Image.MRBase64Image" />
    </feature>
    <feature name="MRClipboard">
        <param name="android-package" value="com.maxrocky.plugins.clipboard.MRClipboard" />
    </feature>
    <feature name="Intent2Web">
        <param name="android-package" value="com.maxrocky.plugins.intent2Web.Intent2Web" />
    </feature>
    <feature name="MRDeviceId">
        <param name="android-package" value="com.maxrocky.plugins.deviceId.MRDeviceId" />
    </feature>
    <feature name="MRUpdateVersion">
        <param name="android-package" value="com.maxrocky.plugins.updateversion.MRUpdateVersion" />
    </feature>
    <feature name="AppVersion">
        <param name="android-package" value="uk.co.whiteoctober.cordova.AppVersion" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <feature name="ImagePicker">
        <param name="android-package" value="com.synconset.ImagePicker" />
    </feature>
    <name>CordovaPluginDemo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
